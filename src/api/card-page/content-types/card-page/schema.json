{
  "kind": "singleType",
  "collectionName": "cards_page",
  "info": {
    "singularName": "card-page",
    "pluralName": "cards-page",
    "displayName": "Карты ФЛ Страница",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "benefits": {
      "displayName": "Преимущество - Карты",
      "type": "component",
      "repeatable": true,
      "component": "components.preimushhestvo-2",
      "min": 3,
      "max": 8,
      "required": true
    },
    "cards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::card.card"
    },
    "additionalServicesTitle": {
      "type": "string",
      "required": true
    },
    "infoAndDocs": {
      "type": "dynamiczone",
      "components": [
        "unit.dokumentami",
        "unit.tekstom",
        "unit.spojlery"
      ],
      "required": true,
      "max": 5,
      "min": 2
    },
    "infoAndDocsTitle": {
      "type": "string",
      "required": true
    },
    "mainBtnText": {
      "type": "string",
      "required": true
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "recommended_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recommended-product.recommended-product"
    },
    "additionalUnit1": {
      "type": "component",
      "repeatable": false,
      "component": "components.malyj-blok",
      "required": true
    },
    "additionalUnit2": {
      "type": "component",
      "repeatable": false,
      "component": "components.malyj-blok",
      "required": true
    },
    "additionalUnit3": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
