{
  "kind": "singleType",
  "collectionName": "cards_page",
  "info": {
    "singularName": "card-page",
    "pluralName": "cards-page",
    "displayName": "Карты ФЛ Страница",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "benefits": {
      "displayName": "Преимущество - Карты",
      "type": "component",
      "repeatable": true,
      "component": "components.preimushhestvo-karty",
      "min": 3,
      "max": 8,
      "required": true
    },
    "cards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::card.card"
    },
    "additionalServicesTitle": {
      "type": "string",
      "required": true
    },
    "InfoAdditionalTitle1": {
      "type": "string",
      "private": true
    },
    "InfoAdditionalTitle2": {
      "type": "string"
    },
    "InfoAdditionalTitle3": {
      "type": "string"
    },
    "firstAdditionalBlockTitle": {
      "type": "string",
      "required": true
    },
    "secondAdditionalBlockTitle": {
      "type": "string",
      "required": true
    },
    "thirdAdditionalBlockTitle": {
      "type": "string",
      "required": true
    },
    "firstAdditionalBtnText": {
      "type": "string",
      "required": true
    },
    "secondAdditionalBtnText": {
      "type": "string",
      "required": true
    },
    "thirdAdditionalBtnText": {
      "type": "string",
      "required": true
    },
    "thirdAdditionalImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "thirdAdditionalDescription": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "InfoAndDocs": {
      "type": "dynamiczone",
      "components": [
        "unit.dokumentami",
        "unit.tekstom"
      ],
      "required": true,
      "max": 5,
      "min": 2
    },
    "AdaptiveUnitTitle": {
      "type": "string",
      "required": true
    },
    "makeCardBtnText": {
      "type": "string",
      "required": true
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
