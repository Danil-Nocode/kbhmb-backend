{
  "kind": "collectionType",
  "collectionName": "mortgages",
  "info": {
    "singularName": "mortgage",
    "pluralName": "mortgages",
    "displayName": "Ипотека",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "mainBtnText": {
      "type": "string",
      "required": true
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "conditions": {
      "type": "component",
      "repeatable": true,
      "component": "components.usloviya",
      "max": 3,
      "min": 3,
      "required": true
    },
    "benefits": {
      "type": "component",
      "repeatable": true,
      "component": "components.preimushhestvo-karty",
      "max": 8,
      "required": true,
      "min": 3
    },
    "calculator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::billing-calculator.billing-calculator"
    },
    "processingMethods": {
      "type": "component",
      "repeatable": false,
      "component": "components.repetitive-with-title",
      "required": true
    },
    "infoAndDocsTitle": {
      "type": "string",
      "required": true
    },
    "infoAndDocs": {
      "type": "dynamiczone",
      "components": [
        "unit.dokumentami",
        "unit.tekstom",
        "unit.spojlery"
      ],
      "required": true,
      "min": 2,
      "max": 5
    },
    "recommended_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recommended-product.recommended-product"
    },
    "banner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    }
  }
}
