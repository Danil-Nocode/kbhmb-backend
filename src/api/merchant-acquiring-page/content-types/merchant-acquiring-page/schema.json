{
  "kind": "singleType",
  "collectionName": "merchant_acquiring_pages",
  "info": {
    "singularName": "merchant-acquiring-page",
    "pluralName": "merchant-acquiring-pages",
    "displayName": "Торговый эквайринг Страница",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "mainImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "mainBtnText": {
      "type": "string",
      "required": true
    },
    "benefits": {
      "type": "component",
      "repeatable": true,
      "component": "components.preimushhestvo-karty",
      "required": true,
      "max": 8,
      "min": 3
    },
    "processingMethods": {
      "type": "component",
      "repeatable": false,
      "component": "components.repetitive-with-title",
      "required": true
    },
    "infoAndDocsTitle": {
      "type": "string",
      "required": true
    },
    "infoAndDocs": {
      "type": "dynamiczone",
      "components": [
        "unit.dokumentami",
        "unit.tekstom"
      ],
      "required": true,
      "max": 5,
      "min": 2
    },
    "banner": {
      "type": "component",
      "repeatable": false,
      "component": "components.bolshoj-blok",
      "required": true
    },
    "recommended_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recommended-product.recommended-product"
    }
  }
}
