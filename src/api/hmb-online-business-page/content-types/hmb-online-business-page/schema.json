{
  "kind": "singleType",
  "collectionName": "hmb_online_business_pages",
  "info": {
    "singularName": "hmb-online-business-page",
    "pluralName": "hmb-online-business-pages",
    "displayName": "ХМБ-онлайн ЮЛ Страница",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "benefits": {
      "type": "component",
      "repeatable": true,
      "required": true,
      "max": 8,
      "min": 3,
      "component": "components.preimushhestvo-2"
    },
    "capabilitiesBlock1": {
      "type": "component",
      "repeatable": false,
      "component": "common.text-description",
      "required": true
    },
    "capabilitiesBlock2": {
      "type": "component",
      "repeatable": false,
      "component": "common.text-description",
      "required": true
    },
    "capabilitiesBlock3": {
      "type": "component",
      "repeatable": false,
      "component": "common.text-description",
      "required": true
    },
    "infoAndDocsTitle": {
      "type": "string",
      "required": true
    },
    "infoAndDocs": {
      "type": "dynamiczone",
      "components": [
        "unit.dokumentami",
        "unit.tekstom",
        "unit.spojlery"
      ],
      "required": true,
      "min": 2,
      "max": 5
    },
    "recommended_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recommended-product.recommended-product"
    },
    "banner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    },
    "appStoreLink": {
      "type": "string",
      "required": false
    },
    "playMarketLink": {
      "type": "string",
      "required": false
    },
    "ruStoreLink": {
      "type": "string"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "capabilitiesTitle": {
      "type": "string"
    }
  }
}
