{
  "kind": "singleType",
  "collectionName": "currency_control_pages",
  "info": {
    "singularName": "currency-control-page",
    "pluralName": "currency-control-pages",
    "displayName": "Валютный контроль Страница",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "infoAndDocsTitle": {
      "type": "string",
      "required": true
    },
    "infoAndDocs": {
      "type": "dynamiczone",
      "components": [
        "unit.dokumentami",
        "unit.tekstom",
        "unit.spojlery"
      ],
      "required": true,
      "min": 2,
      "max": 5
    },
    "recommended_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recommended-product.recommended-product"
    },
    "banner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    }
  }
}
