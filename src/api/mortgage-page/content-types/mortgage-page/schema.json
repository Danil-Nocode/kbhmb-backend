{
  "kind": "singleType",
  "collectionName": "mortgage_pages",
  "info": {
    "singularName": "mortgage-page",
    "pluralName": "mortgage-pages",
    "displayName": "Ипотека Страница",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "mainBtnText": {
      "type": "string",
      "required": true
    },
    "mainImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "mortgages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mortgage.mortgage"
    },
    "calculator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::billing-calculator.billing-calculator"
    },
    "processingMethods": {
      "type": "component",
      "repeatable": false,
      "component": "components.repetitive-with-title",
      "required": true
    },
    "infoAndDocsTitle": {
      "type": "string",
      "required": true
    },
    "infoAndDocs": {
      "type": "dynamiczone",
      "components": [
        "unit.dokumentami",
        "unit.tekstom"
      ],
      "min": 2,
      "max": 5,
      "required": true
    },
    "banner": {
      "type": "component",
      "repeatable": false,
      "component": "components.bolshoj-blok",
      "required": true
    },
    "recommended_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recommended-product.recommended-product"
    }
  }
}
