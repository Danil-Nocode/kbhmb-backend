{
  "kind": "singleType",
  "collectionName": "ibank_pages",
  "info": {
    "singularName": "ibank-page",
    "pluralName": "ibank-pages",
    "displayName": "«Клиент-Банк» (IBank) Страница",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "mainBtnText": {
      "type": "string",
      "required": true
    },
    "benefits": {
      "type": "component",
      "repeatable": true,
      "component": "components.preimushhestvo-2",
      "required": true,
      "max": 8,
      "min": 3
    },
    "capabilitiesBlock1": {
      "displayName": "TextDescription",
      "type": "component",
      "repeatable": false,
      "component": "common.text-description",
      "required": true
    },
    "capabilitiesBlock2": {
      "type": "component",
      "repeatable": false,
      "component": "common.text-description",
      "required": true
    },
    "capabilitiesBlock3": {
      "type": "component",
      "repeatable": false,
      "component": "common.text-description",
      "required": true
    },
    "capabilitiesBlock4": {
      "type": "component",
      "repeatable": false,
      "required": true,
      "component": "common.text-description"
    },
    "capabilitiesBlock5": {
      "type": "component",
      "repeatable": false,
      "required": true,
      "component": "common.text-description"
    },
    "infoAndDocsTitle": {
      "type": "string",
      "required": true
    },
    "infoAndDocs": {
      "type": "dynamiczone",
      "components": [
        "unit.dokumentami",
        "unit.tekstom",
        "unit.spojlery"
      ],
      "required": true,
      "max": 5,
      "min": 2
    },
    "recommended_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recommended-product.recommended-product"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "capabilitiesTitle": {
      "type": "string"
    }
  }
}
